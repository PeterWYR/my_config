{
   "agent": {
      "default_model": {
         "provider": "copilot_chat",
         "model": "claude-opus-4.6",
         "enable_thinking": false,
      },
      "favorite_models": [],
      "model_parameters": [],
   },
   "language_models": {
      "openai_compatible": {
         "MiniMax": {
            "api_url": "https://api.minimax.com/v1",
            "available_models": [
               {
                  "name": "MiniMax-M2.5",
                  "max_tokens": 200000,
                  "max_output_tokens": 32000,
                  "max_completion_tokens": 200000,
                  "capabilities": {
                     "tools": false,
                     "images": false,
                     "parallel_tool_calls": false,
                     "prompt_cache_key": false,
                     "chat_completions": true,
                  },
               },
            ],
         },
      },
   },
   "theme": "Dracula",
   "icon_theme": "Warm Charmed Icons",
   "title_bar": {
      "show_onboarding_banner": false,
      "show_project_items": false,
      "show_branch_name": false,
      "show_user_menu": false,
   },
   "tab_bar": {
      "show": false,
   },
   "toolbar": {
      "quick_actions": false,
   },
   "status_bar": {
      "experimental.show": false,
   },
   "project_panel": {
      "dock": "right",
      "default_width": 400,
      "hide_root": true,
      "auto_fold_dirs": false,
      "starts_open": false,
      "git_status": false,
      "sticky_scroll": false,
      "scrollbar": {
         "show": "never",
      },
      "indent_guides": {
         "show": "never",
      },
   },
   "outline_panel": {
      "default_width": 300,
      "indent_guides": {
         "show": "never",
      },
   },
   "file_finder": {
      "modal_max_width": "large",
   },
   "ui_font_family": "Iosevka",
   "ui_font_size": 20,
   "buffer_font_family": "Iosevka",
   "buffer_font_size": 20,
   "buffer_line_height": {
      "custom": 2,
   },
   "agent_buffer_font_size": 20,
   "vim_mode": true,
   //"multi_cursor_modifier": "cmd_or_ctrl",
   "cursor_shape": "block",
   "drag_and_drop_selection": {
      "enabled": false,
   },
   "tab_size": 3,
   "autosave": {
      "after_delay": {
         "milliseconds": 1000,
      },
   },
   "extend_comment_on_newline": false,
   "horizontal_scroll_margin": 1,
   "vertical_scroll_margin": 1,
   "when_closing_with_no_tabs": "keep_window_open",
   "close_on_file_delete": true,
   "restore_on_file_reopen": false,
   "restore_on_startup": "empty_tab",
   "session": {
      "restore_unsaved_buffers": false,
   },
   "git": {
      "git_gutter": "hide",
      "inline_blame": {
         "enabled": false,
      },
   },
   "centered_layout": {
      "right_padding": 0.15,
      "left_padding": 0.15,
   },
}
